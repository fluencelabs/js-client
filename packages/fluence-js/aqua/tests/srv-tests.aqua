module Export

import Srv from "../src/single-module-srv.aqua"
export test1, test2

service Greeting("greeting"):
    greeting(name: string) -> string

func test1(file_path: string) -> string:
    wasm <- Srv.read_file(file_path)
    id <- Srv.create(wasm)
    Greeting id
    <- Greeting.greeting("test")

func test2(file_path: string) -> string:
    wasm <- Srv.read_file(file_path)
    id <- Srv.create(wasm)
    Greeting id
    <- Greeting.greeting("test")
