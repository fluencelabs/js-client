  "repository": "https://github.com/fluencelabs/fluence-js",
  "author": "Fluence Labs",
  "license": "Apache-2.0",
  "dependencies": {
    "@fluencelabs/interfaces": "0.7.4"
  },
  "devDependencies": {
    "@fluencelabs/avm": "0.35.4",
    "@fluencelabs/marine-js": "0.3.45",
    "microbundle": "0.15.1",
    "dts-bundle-generator": "7.2.0"
  }
}
