{
  "name": "@fluencelabs/js-client.api",
  "version": "0.11.5",
  "description": "JS Client API",
  "engines": {
    "node": ">=10",
    "pnpm": ">=3"
  },
  "type": "module",
  "source": "src/index.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.mjs",
