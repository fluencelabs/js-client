{
  "name": "@fluencelabs/js-client",
  "version": "0.8.0",
  "description": "Client for interacting with Fluence network",
  "engines": {
    "node": ">=10",
    "pnpm": ">=8"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/browser/index.min.js",
