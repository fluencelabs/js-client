{
    "compilerOptions": {
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": false,
        "declaration": true
    }
}
