{
    "name": "@fluencelabs/fluence",
    "version": "0.0.0",
    "description": "TypeScript implementation of Fluence Peer",
    "exports": "./dist/index.js",
    "typings": "./dist/index.d.ts",
    "engines": {
        "node": ">=16.0"
    },
    "scripts": {
        "compile-aqua": "npm run compile-aqua:src && npm run compile-aqua:test",
        "compile-aqua:src": "aqua -i ./aqua/src/  -o ./src/internal/_aqua",
        "compile-aqua:test": "aqua -i ./aqua/tests/ -o ./src/__test__/_aqua",
        "test": "jest --watch",
        "test:all": "jest",
        "test:unit": "jest --testPathPattern=src/__test__/unit",
        "test:integration": "jest --testPathPattern=src/__test__/integration",
        "build": "tsc",
        "build:docs": "typedoc"
    },
    "repository": "https://github.com/fluencelabs/fluence-js",
    "author": "Fluence Labs",
    "license": "Apache-2.0",
    "bin": {
        "copy-marine": "dist/copyMarine.js"
    },
    "dependencies": {
        "@chainsafe/libp2p-noise": "^6.1.1",
        "@fluencelabs/avm": "0.24.2",
        "@fluencelabs/marine-js": "0.2.5",
        "@libp2p/mplex": "^1.0.5",
        "@libp2p/peer-id": "^1.1.10",
        "@libp2p/websockets": "^1.0.8",
        "@multiformats/multiaddr": "^10.1.8",
        "async": "3.2.3",
        "browser-or-node": "^2.0.0",
        "bs58": "5.0.0",
        "buffer": "^6.0.3",
        "cids": "1.1.9",
        "it-length-prefixed": "5.0.3",
        "it-pipe": "1.1.0",
        "libp2p": "0.37.0",
        "libp2p-crypto": "0.21.2",
        "loglevel": "1.8.0",
        "platform": "^1.3.6",
        "rxjs": "^7.5.5",
        "ts-pattern": "3.3.3",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@fluencelabs/aqua": "^0.7.2-307",
        "@fluencelabs/aqua-lib": "^0.5.1",
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@types/bs58": "^4.0.1",
        "@types/jest": "^27.5.1",
        "@types/platform": "^1.3.4",
        "@types/uuid": "^8.3.4",
        "jest": "^28.1.0",
        "js-base64": "^3.7.2",
        "rollup": "^2.74.1",
        "rollup-plugin-clear": "^2.0.7",
        "rollup-plugin-preserve-shebang": "^1.0.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-ts": "^2.0.7",
        "ts-jest": "^28.0.2",
        "typedoc": "^0.22.15",
        "typescript": "^4.6.4"
    }
}
